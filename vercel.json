{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/config.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/config",
            "dest": "/api/config.js"
        },
        {
            "src": "/(.*).(js|css|map)",
            "dest": "/public/$1.$2"
        },
        {
            "src": "/.*",
            "dest": "/public/index.html"
        }
    ]
}